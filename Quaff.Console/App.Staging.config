<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <appSettings>
        <add key="ASD_FILE_SOURCE_FOLDER" value="ADD_VALUE"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
        <add key="FILE_DOWNLOAD_PATH"     value="ADD_VALUE"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
        <add key="MAIL_ISCHOOL_ADDRESS"   value="ADD_VALUE"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
        <add key="FILE_ARCHIVE_PATH"      value="ADD_VALUE"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
    </appSettings>
    <connectionStrings>
        <add name="ZangleContext" connectionString="ADD_VALUE"
             providerName="System.Data.SqlClient"
            xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
</configuration>